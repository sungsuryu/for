<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE form-validation PUBLIC 
    "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1//EN" 
    "http://jakarta.apache.org/commons/dtds/validator_1_1.dtd">

<form-validation>

	<formset>
		<form name="loginVO">
			<field property="loginId" depends="required">
				<arg0 key="title.login.loginid" />
			</field>
			<field property="password" depends="required">
				<arg0 key="title.login.password" />
			</field>
		</form>
		
		<form name="userVO">
			<field property="userId" depends="required">
				<arg0 key="title.login.loginid" />
			</field>
			<field property="password" depends="required">
				<arg0 key="title.join.password" />
			</field>
			<field property="userNm" depends="required">
				<arg0 key="title.join.userNm" />
			</field>
			<field property="insurCd" depends="required">
				<arg0 key="title.join.insurCd" />
			</field>
			<field property="joinAgree" depends="required">
				<arg0 key="title.login.joinAgree" />
			</field>
		</form>
	</formset>
</form-validation>